{
  "ServerProxy": {
    "servers": {
      "jupyterlite-py": {
        "command": ["yarn", "serve:py", "{port}"],
        "launcher_entry": {
          "icon_path": "/home/jovyan/docs/_static/icon.svg",
          "title": "JupyterLite (py)"
        }
      },
      "jupyterlite-js": {
        "command": ["yarn", "serve", "--port", "{port}"],
        "launcher_entry": {
          "icon_path": "/home/jovyan/docs/_static/icon.svg",
          "title": "JupyterLite (js)"
        }
      }
    }
  }
}
